{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch App",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}/src/index.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Attach to App",
      "type": "pwa-node",
      "request": "attach",
      "restart": true,
      "port": 9229
    }
  ]
}